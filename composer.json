{
  "name": "hagig79/fuelphp-multi-process",
  "description": "",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": ">=5.4",
    "composer/installers": "~1.0",
    "fuel/core": "1.8.*",
    "fuel/auth": "1.8.*",
    "fuel/email": "1.8.*",
    "fuel/oil": "1.8.*",
    "fuel/orm": "1.8.*",
    "fuel/parser": "1.8.*",
    "fuelphp/upload": "2.0.6"
  },
  "config": {
    "vendor-dir": "fuel/vendor"
  },
  "extra": {
    "installer-paths": {
      "fuel/{$name}": [
        "fuel/core"
      ],
      "{$name}": [
        "fuel/docs"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "php oil r install"
    ]
  },
  "minimum-stability": "stable"
}
